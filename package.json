{
  "name": "@nekuda/acp-test",
  "version": "0.1.1",
  "description": "CLI tool to test Agentic Commerce Protocol implementations",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "acp-test": "./dist/cli.js"
  },
  "files": [
    "dist",
    "spec"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
    "prebuild": "mkdir -p spec",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "keywords": [
    "acp",
    "agentic-commerce",
    "testing",
    "cli",
    "validator",
    "checkout",
    "openai"
  ],
  "author": "Nekuda <hello@nekuda.ai> (https://nekuda.ai)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nekuda-ai/acp-validator-cli.git"
  },
  "bugs": {
    "url": "https://github.com/nekuda-ai/acp-validator-cli/issues"
  },
  "homepage": "https://github.com/nekuda-ai/acp-validator-cli#readme",
  "dependencies": {
    "@inquirer/prompts": "^6.0.0",
    "@seriousme/openapi-schema-validator": "^2.7.0",
    "ajv": "^8.17.0",
    "axios": "^1.7.0",
    "chalk": "^5.6.2",
    "cli-spinners": "^3.3.0",
    "cosmiconfig": "^9.0.0",
    "handlebars": "^4.7.8",
    "ink": "^6.3.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-task-list": "^2.0.0",
    "js-yaml": "^4.1.0",
    "react": "^19.2.0",
    "supertest": "^7.1.4",
    "vitest": "^3.1.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/ink-box": "^1.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.2",
    "@types/supertest": "^6.0.3",
    "@types/yargs": "^17.0.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.0.0",
    "prettier": "^3.5.0",
    "tsup": "^8.3.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0"
  }
}
